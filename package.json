{
  "name": "snapdragon-lexer",
  "description": "Converts a string into an array of tokens, with useful methods for looking ahead and behind, capturing, matching, et cetera.",
  "version": "3.1.0",
  "homepage": "https://github.com/here-be/snapdragon-lexer",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "here-be/snapdragon-lexer",
  "bugs": {
    "url": "https://github.com/here-be/snapdragon-lexer/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "nyc mocha"
  },
  "devDependencies": {
    "define-property": "^2.0.2",
    "gulp-format-md": "^1.0.0",
    "mocha": "^3.5.3",
    "snapdragon-position": "^2.0.2"
  },
  "keywords": [
    "compile",
    "compiler",
    "convert",
    "lexer",
    "parse",
    "parser",
    "render",
    "scan",
    "scanner",
    "snapdragon",
    "token",
    "tokenize",
    "tokenizer",
    "transform"
  ],
  "nyc": {
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "verb": {
    "toc": "collapsible",
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": [
        "snapdragon-node",
        "snapdragon-position",
        "snapdragon-token"
      ]
    },
    "reflinks": [
      "snapdragon-location",
      "snapdragon-parser",
      "snapdragon-position",
      "snapdragon-token"
    ]
  }
}
